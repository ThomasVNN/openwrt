#include "qcom-ipq4019.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/soc/qcom,tcsr.h>

/ {
	model = "Nokia Airscale WI2B-AC220i";
	compatible = "nokia,wi2b-ac220i", "qcom,ipq4019";

	aliases {
		led-boot = &pwr_red;
		led-upgrade = &pwr_green;
		led-failsafe = &pwr_red;
		led-running = &pwr_green;
	};

	chosen {
		bootargs-append = " ubi.mtd=rootfs ubi.block=0,1 root=/dev/ubiblock0_1 rootfstype=squashfs ro clk_ignore_unused";

		stdout-path = "serial0:115200n8";
	};

	memory {
		device_type = "memory";
		reg = <0x80000000 0x20000000>; /* 512MB */
	};

	soc {

		mdio@90000 {
			status = "okay";
			pinctrl-0 = <&mdio_pins>;
			pinctrl-names = "default";

			/delete-node/ ethernet-phy@0;
			/delete-node/ ethernet-phy@1;
			/delete-node/ ethernet-phy@2;
			/delete-node/ ethernet-phy@3;
			/delete-node/ ethernet-phy@4;

			ethernet-phy@5 {
				reg = <0x5>;
			};
		};

		rng@22000 {
			status = "okay";
		};

		crypto@8e3a000 {
			status = "okay";
		};

		watchdog@b017000 {
			status = "okay";
		};

		ess-switch@c000000 {
			switch_mac_mode = <0x3>;
			switch_lan_bmp = <0x0>;
			switch_wan_bmp = <0x10>;
		};

		edma@c080000 {
			qcom,single-phy;
			qcom,num_gmac = <1>;
			phy-mode = "rgmii-id";
			status = "okay";
		};
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			gpios = <&tlmm 50 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};

	leds {
		compatible = "gpio-leds";

		pinctrl-0 = <&led_pins>;
		pinctrl-names = "default";

		wlan5g_green {
			label = "ac220i:green:wlan5g";
			gpios = <&tlmm 46 GPIO_ACTIVE_LOW>;
		};

		wlan5g_red {
			label = "ac220i:red:wlan5g";
			gpios = <&tlmm 45 GPIO_ACTIVE_LOW>;
		};

		wlan2g_green {
			label = "ac220i:green:wlan2g";
			gpios = <&tlmm 40 GPIO_ACTIVE_LOW>;
		};

		wlan2g_red {
			label = "ac220i:red:wlan2g";
			gpios = <&tlmm 39 GPIO_ACTIVE_LOW>;
		};

		eth_green {
			label = "ac220i:green:eth";
			gpios = <&tlmm 48 GPIO_ACTIVE_LOW>;
		};

		eth_red {
			label = "ac220i:red:eth";
			gpios = <&tlmm 47 GPIO_ACTIVE_LOW>;
		};

		ctrl_green {
			label = "ac220i:green:ctrl";
			gpios = <&tlmm 43 GPIO_ACTIVE_LOW>;
		};

		ctrl_red {
			label = "ac220i:red:ctrl";
			gpios = <&tlmm 42 GPIO_ACTIVE_LOW>;
		};

		pwr_green: pwr_green {
			label = "ac220i:green:pwr";
			gpios = <&tlmm 52 GPIO_ACTIVE_LOW>;
		};

		pwr_red: pwr_red {
			label = "ac220i:red:pwr";
			gpios = <&tlmm 49 GPIO_ACTIVE_LOW>;
			panic-indicator;
		};
	};
};

&tlmm {

	mdio_pins: mdio_pinmux {

		mux_1 {
			pins = "gpio6";
			function = "mdio";
			bias-pull-up;
		};

		mux_2 {
			pins = "gpio7";
			function = "mdc";
			bias-pull-up;
		};

	};

	serial_0_pins: serial0-pinmux {
		pins = "gpio16", "gpio17";
		function = "blsp_uart0";
		bias-disable;
	};

	spi_0_pins: spi-0-pinmux {

		pinmux {
			function = "blsp_spi0";
			pins = "gpio13", "gpio14", "gpio15";
			bias-disable;
		};

		pinmux_cs {
			function = "gpio";
			pins = "gpio12";
			bias-disable;
			output-high;
		};

	};

	led_pins: led_pins {

		mux {
			pins = "gpio46", "gpio45", "gpio40", "gpio39", "gpio48",
			       "gpio47", "gpio43", "gpio42", "gpio52", "gpio49";
			function = "gpio";
			drive-strength = <2>;
			bias-pull-down;
		};

	};
};

&blsp1_uart1 {
	pinctrl-0 = <&serial_0_pins>;
	pinctrl-names = "default";
	status = "ok";
};

&blsp_dma {
	status = "okay";
};

&wifi0 {
	status = "okay";
};

&wifi1 {
	status = "okay";
};

&cryptobam {
	status = "okay";
};

&gmac0 {
	qcom,phy_mdio_addr = <5>;
	qcom,poll_required = <1>;
	vlan_tag = <0 0x20>;
};

&blsp1_spi1 {
	pinctrl-0 = <&spi_0_pins>;
	pinctrl-names = "default";
	status = "ok";
	cs-gpios = <&tlmm 12 0>;

	w25q32@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		reg = <0>;
		compatible = "w25q32";
		spi-max-frequency = <24000000>;

		partitions {
			compatible = "fixed-partitions";

			SBL1@0 {
				label = "0:SBL1";
				reg = <0x00000000 0x00040000>;
				read-only;
			};

			MIBIB@40000 {
				label = "0:MIBIB";
				reg = <0x00040000 0x00020000>;
				read-only;
			};

			BOOTCONFIG@60000 {
				label = "0:BOOTCONFIG";
				reg = <0x00060000 0x00020000>;
			};

			QSEE_1@80000 {
				label = "0:QSEE_1";
				reg = <0x00080000 0x00060000>;
				read-only;
			};

			QSEE@e0000 {
				label = "0:QSEE";
				reg = <0x000e0000 0x00060000>;
				read-only;
			};

			CDT_1@140000 {
				label = "0:CDT_1";
				reg = <0x00140000 0x00010000>;
				read-only;
			};

			CDT@150000 {
				label = "0:CDT";
				reg = <0x00150000 0x00010000>;
				read-only;
			};

			BOOTCONFIG1@160000 {
				label = "0:BOOTCONFIG1";
				reg = <0x00160000 0x00020000>;
			};

			DDRPARAMS@180000 {
				label = "0:DDRPARAMS";
				reg = <0x00180000 0x00010000>;
				read-only;
			};

			APPSBLENV@190000 {
				label = "0:APPSBLENV";
				reg = <0x00190000 0x00010000>;
			};

			APPSBL_1@1a0000 {
				label = "0:APPSBL_1";
				reg = <0x001a0000 0x00080000>;
				read-only;
			};

			APPSBL@220000 {
				label = "0:APPSBL";
				reg = <0x00220000 0x00080000>;
				read-only;
			};

			RES1@2a0000 {
				label = "0:RES1";
				reg = <0x002a0000 0x00130000>;
				read-only;
			};

			ART@3d0000 {
				label = "0:ART";
				reg = <0x003d0000 0x00010000>;
				read-only;
			};

			EID-A@3e0000 {
				label = "0:EID-A";
				reg = <0x003e0000 0x00010000>;
				read-only;
			};

			EID-B@3f0000 {
				label = "0:EID-B";
				reg = <0x003f0000 0x00010000>;
				read-only;
			};
		};

	};
};

&qpic_bam {
	status = "okay";
};

&nand {
	status = "okay";

	nand@0 {
		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			rootfs@0 {
				label = "rootfs";
				reg = <0x00000000 0x03000000>;
			};

			rootfs_1@3000000 {
				label = "rootfs_1";
				reg = <0x03000000 0x03000000>;
			};

			cfg@6000000 {
				label = "cfg";
				reg = <0x06000000 0x02000000>;
			};

		};

	};
};
